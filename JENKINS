pipeline {
    agent any{
        label 'python'
        }
    stages {
        stage('Version') {
            steps {
                sh 'python3 --version'
            }
        }
        stage('build'){
            steps{
                sh 'python3 tuples.py'

            }
        }
        stage('Test'){
            steps{
                echo 'File is tested'
            }
        }
    }
}
